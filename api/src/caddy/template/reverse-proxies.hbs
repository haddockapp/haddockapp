{{#each data}}
{{hostname}} {
	reverse_proxy * {{ip}}:{{port}}
}
{{/each}}
{{#if frontend}}
{{frontend.hostname}} {
{{#if frontend.https}}
    reverse_proxy * {{frontend.ip}}:{{frontend.port}} {
        header_up Host {upstream_hostport}
    }
{{else}}
    root * {{frontend.root}}
    try_files {path} /index.html
    file_server
{{/if}}
}
{{/if}}
